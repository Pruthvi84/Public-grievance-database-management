<%- include("partials/header"); -%>
<form  action="index.html" method="post">
  <div class="form-group">
    <label for="UserId">UserId</label>
    <input name="UserIdWB" type="text" class="form-control" id="UserID" aria-describedby="UserHelp" placeholder="Enter your UserID" required>
    <small id="UserHelp" class="form-text text-muted">Your userId will be confidential,will not be shared with anyone</small>
  </div>
  <div class="form-group">
    <label for="Address">Address</label>
    <input type="text" name="AddressRoadMain" class="form-control" id="Address" placeholder="Address" required>
  </div>
  <div class="form-group">
    <label for="Landmark">Nearest Landmark</label>
    <input list="Landmark" name="Landmark" class="form-control" placeholder="Nearest Landmark">
    <datalist  id="Landmark">

      <option value="JP Nagar">
      <option value="VishveshwaraNagar">
      <option value="Bogadi">
      <option value="SiddharthNagar">
      <option value="Ramkrishna Nagar">
      <option value="Srirampura">
     </datalist>
  </div>
  <div class="form-group">
    <label for="Pincode">Pincode</label>
    <input type="text" name="Pincode" class="form-control" id="Pincode" placeholder="Pincode" required>
  </div>
  <div class="form-group">
    <label for="Area">Area of stay</label>
    <input type="text" name="Area" class="form-control" id="Area" placeholder="Area of Stay" required>
  </div>
  <div class="form-group">
  <label for="QualityOfRoad">Quality Of Road</label><br>

   <div class="form-check form-check-inline mb-0 me-4">
   <input class="form-check-input" type="radio" name="QualityOfRoad" id="Abovepar" value="Abovepar" required/>
   <label class="form-check-label" for="Abovepar">Above par</label>
   </div>

<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="QualityOfRoad" id="Belowpar" value="Belowpar" required/>
<label class="form-check-label" for="Belowpar">Below Par</label>
</div>

<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="QualityOfRoad" id="Worst" value="Worst" required/>
<label class="form-check-label" for="Worst">Worst</label>
</div>
</div>
<div class="form-group">
<label for="exampleFormControlTextarea1">Use this TextBox to kindly inform us more about the complaints you are facing </label>
<textarea class="form-control" name="textRM" id="exampleFormControlTextarea1" rows="3"></textarea>
</div>
<button type="submit" class="btn btn-primary">Submit</button>
</form>
<%- include("partials/footer"); -%>
