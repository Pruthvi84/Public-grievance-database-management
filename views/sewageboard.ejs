  <%- include("partials/header"); -%>
    <form method="post" action="/sewageboard" class="formarea">
  <div class="form-group">
    <label for="UserId">UserId</label>
    <input type="text" class="form-control" id="UserID" aria-describedby="UserHelp" placeholder="Enter your UserID">
    <small id="UserHelp" class="form-text text-muted">Your userId will be confidential,will not be shared with anyone</small>
  </div>
  <div class="form-group">
    <label for="Address">Address</label>
    <input type="text" class="form-control" id="Address" placeholder="Address">
  </div>
  <div class="form-group">
    <label for="Landmark">Nearest Landmark</label>
    <input list="Landmark" name="Landmark" class="form-control" placeholder="Nearest Landmark">
    <datalist  id="Landmark">

      <option value="JP Nagar">
      <option value="VishveshwaraNagar">
      <option value="Bogadi">
      <option value="SiddharthNagar">
      <option value="Ramkrishna Nagar">
      <option value="Srirampura">
     </datalist>
  </div>
  <div class="form-group">
  <label for="ManholeIssue">Is your compain related to Manhole Issue?</label><br>
  <div class="form-check form-check-inline mb-0 me-4">
  <input class="form-check-input" type="radio" name="ManholeIssue" id="YesManhole" value="Yes" required/>
  <label class="form-check-label" for="YesManhole">Yes</label>
  </div>

  <div class="form-check form-check-inline mb-0 me-4">
  <input class="form-check-input" type="radio" name="ManholeIssue" id="NoManhole" value="No" required/>
  <label class="form-check-label" for="NoManhole">No</label>
  </div>
</div>
<div class="form-group">
<label for="DrainageIssue">Is your complain related to Drainage issues?</label><br>
<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="DrainageIssue" id="YesDrainage" value="Yes" required/>
<label class="form-check-label" for="YesDrainage">Yes</label>
</div>

<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="DrainageIssue" id="NoDraiange" value="No" required/>
<label class="form-check-label" for="NoDrainage">No</label>
</div>
</div>
<div class="form-group">
<label for="Garbage">Is your complain related to Garbage Collection and Disposal?</label><br>
<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="Garbage" id="YesGarbage" value="Yes" required/>
<label class="form-check-label" for="YesGarbage">Yes</label>
</div>

<div class="form-check form-check-inline mb-0 me-4">
<input class="form-check-input" type="radio" name="Garbage" id="NoGarbage" value="No" required/>
<label class="form-check-label" for="NoGarbage">No</label>
</div>
</div>
<div class="form-group">
    <label for="exampleFormControlTextarea1">Use this TextBox to kindly inform us more about the complaints you have at your place </label>
    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
<%- include("partials/footer"); -%>
